{
  "name": "create-pop-react-app",
  "version": "1.0.3",
  "keywords": [
    "react",
    "next",
    "next.js"
  ],
  "description": "Create Next.js-powered React apps with one command",
  "repository": {
    "type": "git",
    "url": "https://github.com/cyber-echo/create-pop-react-app"
  },
  "author": "cyber-echo",
  "license": "MIT",
  "bin": {
    "create-pop-react-app": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ncc build ./index.ts -w -o dist/",
    "prerelease": "node ./rm.mjs dist",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "build": "npm release",
    "prepublishOnly": "zx ./scripts/prepublish.mjs",
    "postpublish": "zx ./scripts/postpublish.mjs",
    "lint-fix": "npm prettier -w --plugin prettier-plugin-tailwindcss 'templates/*-tw/{ts,js}/{app,pages}/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "@types/async-retry": "1.4.2",
    "@types/ci-info": "2.0.0",
    "@types/cross-spawn": "6.0.0",
    "@types/eslint": "^8.40.1",
    "@types/glob": "7.1.1",
    "@types/node": "^20.2.5",
    "@types/prettier": "^2.7.3",
    "@types/prompts": "2.0.1",
    "@types/tar": "6.1.5",
    "@types/validate-npm-package-name": "3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "@vercel/ncc": "0.34.0",
    "async-retry": "1.3.1",
    "async-sema": "^3.1.1",
    "chalk": "2.4.2",
    "ci-info": "watson/ci-info#f43f6a1cefff47fb361c88cf4b943fdbcaafe540",
    "commander": "2.20.0",
    "conf": "10.2.0",
    "cpy": "7.3.0",
    "cross-spawn": "6.0.5",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "glob": "8.0.3",
    "got": "10.7.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "prompts": "2.1.0",
    "tar": "6.1.15",
    "update-check": "1.5.4",
    "validate-npm-package-name": "3.0.0",
    "zx": "^7.2.2"
  },
  "engines": {
    "node": ">=16.8.0"
  }
}
